<h1>編集画面</h1>
<!-- form_with部分 -->
<%= form_with model:@book, url: book_path, local: true do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4>
  <%= f.text_area :body %>
  <%= f.submit 'Update Book' %>
<% end %>

<p><%= link_to "SHOW", book_path %></p>
<p><%= link_to "BACK", books_path %></p>